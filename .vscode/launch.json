    "version": "0.2.0",
    "configurations": [
        {
            "name": "Test Probability",
            "type": "python",
            "request": "launch",
            "program": "pythonic_implementation.py",
            "console": "integratedTerminal",
            "args": [
                "-b", "1",
                "-p", "2",
                "-i", "points.txt",
                "-n", "edges.txt",
                "-a", "3",
                "-c", "10",
                "-d", "2",
                "-k", "10"
            ],
            "cwd": "/mnt/c/Users/dmalj/projects/active/nglpy_cuda/data"
        }
        {
            "name": "Python: Current File (Integrated Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Attach",
            "type": "python",
            "request": "attach",
            "port": 5678,
            "host": "localhost"
        },
        {
            "name": "Python: Django",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "console": "integratedTerminal",
            "args": [
                "runserver",
                "--noreload",
                "--nothreading"
            ],
            "django": true
        },
        {
            "name": "Python: Flask",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "app.py"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": true
        },
        {
            "name": "Python: Current File (External Terminal)",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "externalTerminal"
        }
    ]
}